{
    "manifest_version":2,
    "name": "Happy22",
    "version": "0.1",
    "web_accessible_resources": [
      "assets/*.jpg"
    ],
    "content_scripts" : [{
    "matches" : ["<all_urls>"],
    "js" : ["content.js"]
    }],
    "browser_action": {
    "default_icon": "sunglassic.png",
    "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}
 
    
